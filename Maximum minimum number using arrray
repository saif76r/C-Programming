#include<stdio.h>
int main(){
    int i,n;
    scanf("%d",&n);
    
    int a[n];
    
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    
    }
    
    int max=a[0];
    int min=a[0];
    
    for(i=0;i<n;i++){
        if(a[i]>max){
            max = a[i];
            
        }
        if(a[i]<min){
            min = a[i];
        }
    }
       printf("Maximum element = %d\n", max);
    printf("Minimum element = %d\n", min);

}
