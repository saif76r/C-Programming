#include<stdio.h>
int loop(int n){
    int i;
    printf("Number is ");
    for (i=0;i<n;i++){
        printf("%d\n",i);
    }
    return n;
}

int even(int n){
int i;
printf("Even num is ");
for(i=1;i<n;i++){
    if(i%2 == 0){
        
        printf("%d\n",i);
    }
    
}
return n;
}


int main(){
    int i,n,r,e;
    scanf("%d",&n);
    
    e=even(n);
    r=loop(n);
}
