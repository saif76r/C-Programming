#include <stdio.h>

int main() {
    int i, n;
    scanf("%d", &n);

    int a[n];

    // Read numbers and replace negatives with 0
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
        if (a[i] < 0) {
            a[i] = 0;
        }
    }

    // Print modified numbers
    for (i = 0; i < n; i++) {
        printf("%d ", a[i]);
    }
    printf("\n");

    return 0;
}
