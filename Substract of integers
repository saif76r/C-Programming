
#include <stdio.h>

int main() {
    int a, b, sub;

    // Ask user to enter two integers
    printf("Enter first number: ");
    scanf("%d", &a);

    printf("Enter second number: ");
    scanf("%d", &b);

    // Calculate sum
    sum = a - b;

    // Print result
    printf("Sub = %d\n", sub);

    return 0;
}
