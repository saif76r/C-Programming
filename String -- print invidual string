#include <stdio.h>
#include <string.h>

int main() {
    char a[50];
    int i;

    printf("Enter a string: ");
    fgets(a, sizeof(a), stdin);

    // Remove newline character from fgets
    a[strcspn(a, "\n")] = '\0';

    for (i = 0; a[i] != '\0'; i++) {
        printf("Character %d: %c\n",i+1 , a[i]);
    }

    return 0;
}

