#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int freq[256] = {0};  // frequency array for ASCII characters
    int i, max = -1;
    char result;

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    // remove newline 
    str[strcspn(str, "\n")] = '\0';

    // count frequency of each character
    for (i = 0; str[i] != '\0'; i++) {
        freq[(unsigned char)str[i]]++;
    }

    // find max occurring character
    for (i = 0; i < 256; i++) {
        if (freq[i] > max) {
            max = freq[i];
            result = (char)i;
        }
    }

    printf("Maximum occurring character: '%c' occurred %d times\n", result, max);

    return 0;
}
