#include <stdio.h>
#include <string.h>

int main() {
    char a[100], b[100], r[300];
    int i = 0, j = 0;

    printf("Enter first string: ");
    fgets(a, sizeof(a), stdin);
    if(a[strlen(a)-1] == '\n') a[strlen(a)-1] = '\0'; // remove newline

    printf("Enter second string: ");
    fgets(b, sizeof(b), stdin);
    if(b[strlen(b)-1] == '\n') b[strlen(b)-1] = '\0'; // remove newline

    // Copy first string
    while(a[i] != '\0') {
        r[i] = a[i];
        i++;
    }

    // Copy second string
    while(b[j] != '\0') {
        r[i] = b[j];
        i++;
        j++;
    }

    // Null-terminate result
    r[i] = '\0';

    printf("Concatenated string: %s\n", r);

    return 0;
}
