#include <stdio.h>

int main() {
    int n, i;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int a[n];
    printf("Enter %d numbers: ", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    printf("Odd numbers: ");
    for (i = 0; i < n; i++) {
        if (a[i] % 2 = 1) {
            printf("%d ", a[i]);
        }
    }
    printf("\n");

    return 0;
}
