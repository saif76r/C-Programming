
#include<stdio.h>
int main(){

int i,n,prime=1;

printf("Enter a number : ");
scanf("%d",&n);

for(i=2;i<n;i++){

if(n%i == 0)
    prime = 0;

}
if (n==1)
   printf("Not prime number");

else if(prime == 1)
   printf("prime number");

else if(prime == 0)
   printf("Not a prime number");

   return 0;
}

