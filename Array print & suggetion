#include<stdio.h>
int main(){
    int i,n;
    scanf("%d",&n);
    
    #include<stdio.h>
int a[n];
    int sum=0;
    
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    
    
    for(i=0;i<n;i++){
        printf("%d",a[i]);
        sum += a[i];
        
    }
    printf("Sum is : %d",sum);
    
}
