
#include <stdio.h>

int main() {
    int a, b, div;

    // Ask user to enter two integers
    printf("Enter first number: ");
    scanf("%d", &a);

    printf("Enter second number: ");
    scanf("%d", &b);

    // Calculate div
    div = a / b;

    // Print result
    printf("Div = %d\n", div);

    return 0;
}
